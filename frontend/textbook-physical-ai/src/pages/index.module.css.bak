/**
 * CSS files with the .module.css suffix will be treated as CSS modules
 * and scoped locally.
 */

.heroSection {
  padding: 4rem 0;
  min-height: 100vh;
  display: flex;
  align-items: center;
  background: linear-gradient(135deg, #0f172a 0%, #1e293b 100%);
  color: #f1f5f9;
}

@media screen and (max-width: 996px) {
  .heroSection {
    padding: 2rem 0;
    min-height: auto;
  }
}

.heroContent {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 3rem;
  width: 100%;
}

@media screen and (max-width: 996px) {
  .heroContent {
    flex-direction: column;
    text-align: center;
    gap: 2rem;
  }
}

.textContent {
  flex: 1;
  max-width: 600px;
}

@media screen and (max-width: 996px) {
  .textContent {
    max-width: 100%;
  }
}

.mainTitle {
  font-family: 'Georgia', 'Times New Roman', serif;
  font-size: 2.5rem;
  font-weight: 700;
  margin-bottom: 1rem;
  line-height: 1.2;
  color: #c084fc;
  text-transform: uppercase;
  letter-spacing: 1.5px;
  text-shadow: 0 0 10px rgba(192, 132, 252, 0.5);
}

@media screen and (max-width: 996px) {
  .mainTitle {
    font-size: 2rem;
  }
}

.subTitle {
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  font-size: 1.8rem;
  font-weight: 500;
  margin-bottom: 1.5rem;
  color: #a5b4fc;
  line-height: 1.3;
}

@media screen and (max-width: 996px) {
  .subTitle {
    font-size: 1.5rem;
  }
}

.description {
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 1.1rem;
  margin-bottom: 2rem;
  color: #cbd5e1;
  line-height: 1.6;
}

.buttons {
  display: flex;
  gap: 1rem;
  margin-bottom: 2rem;
  flex-wrap: wrap;
}

@media screen and (max-width: 996px) {
  .buttons {
    flex-direction: column;
    align-items: center;
  }
}

.features {
  display: flex;
  gap: 2rem;
  align-items: center;
}

@media screen and (max-width: 996px) {
  .features {
    justify-content: center;
    flex-wrap: wrap;
  }
}

.featureItem {
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.featureIcon {
  font-size: 1.5rem;
}

.featureText {
  color: #94a3b8;
  font-size: 0.9rem;
}

.illustration {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  min-width: 500px;
  min-height: 500px;
}

.futuristic-robot-container {
  width: 500px;
  height: 500px;
  display: flex;
  align-items: center;
  justify-content: center;
}

@media screen and (max-width: 996px) {
  .illustration {
    min-width: 100%;
    min-height: 300px;
    margin-top: 1rem;
  }

  .futuristic-robot-container {
    width: 300px;
    height: 300px;
  }
}

.robotIllustration {
  width: 100%;
  max-width: 300px;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.robotSvg {
  width: 100%;
  height: auto;
  filter: drop-shadow(0 10px 15px rgba(192, 132, 252, 0.3));
}

/* Animation for rotating orbits */
@keyframes rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

@keyframes orbit {
  0% {
    transform: rotate(0deg) translateX(100px) rotate(0deg);
  }
  100% {
    transform: rotate(360deg) translateX(100px) rotate(-360deg);
  }
}

.orbitGroup {
  transform-origin: center;
  animation: rotate 20s linear infinite;
}

.orbitCircle {
  transform-origin: center;
}

.orbitParticle {
  transform-origin: center;
}

/* Keyframes for rotating orbits */
@keyframes rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

@keyframes orbit {
  0% {
    transform: rotate(0deg) translateX(100px) rotate(0deg);
  }
  100% {
    transform: rotate(360deg) translateX(100px) rotate(-360deg);
  }
}

@keyframes pulse {
  0%, 100% {
    opacity: 0.8;
    transform: scale(1);
  }
  50% {
    opacity: 1;
    transform: scale(1.3);
  }
}

@keyframes orbitMove {
  0% {
    transform: rotate(0deg) translateX(100px) rotate(0deg);
  }
  100% {
    transform: rotate(360deg) translateX(100px) rotate(-360deg);
  }
}

.orbitGroup {
  transform-origin: center;
  animation: rotate 20s linear infinite;
}

.orbitCircle {
  transform-origin: center;
}

.movementDot {
  transform-origin: 200px 200px;
}

/* Classes for the animated circles around the robot */
.mainOrbit {
  transform-origin: 200px 230px;
  animation: rotate 30s linear infinite;
}

.secondaryOrbit {
  transform-origin: 200px 230px;
  animation: rotate 25s linear infinite reverse;
}

.orbitParticle {
  transform-origin: 200px 230px;
}

.counterParticle {
  transform-origin: 200px 230px;
}

/* Keyframes for moving spots on robot */
@keyframes moveSpot1 {
  0%, 100% { cx: 180; cy: 230; }
  50% { cx: 185; cy: 225; }
}

@keyframes moveSpot2 {
  0%, 100% { cx: 220; cy: 250; }
  50% { cx: 215; cy: 255; }
}

@keyframes moveSpot3 {
  0%, 100% { cx: 200; cy: 270; }
  50% { cx: 195; cy: 265; }
}

@keyframes moveSpot4 {
  0%, 100% { cx: 190; cy: 210; }
  50% { cx: 195; cy: 215; }
}

.movingSpot {
  animation-duration: 4s;
  animation-iteration-count: infinite;
  animation-timing-function: ease-in-out;
}

/* Responsive Design Adjustments */
@media (max-width: 768px) {
  .heroBanner {
    padding: 2rem 1rem;
  }

  .heroInner {
    flex-direction: column;
    text-align: center;
  }

  .textContent {
    max-width: 100%;
    margin-bottom: 2rem;
  }

  .illustration {
    min-width: 100%;
    margin-top: 2rem;
  }

  .robotIllustration {
    max-width: 100%;
    height: auto;
  }

  .robotSvg {
    width: 100%;
    height: auto;
    max-width: 300px; /* Reduced max-width for mobile */
  }

  .mainTitle {
    font-size: 2rem;
  }

  .subTitle {
    font-size: 1.3rem;
  }

  .buttons {
    flex-direction: column;
    gap: 1rem;
  }

  .features {
    flex-direction: column;
    align-items: center;
    gap: 1rem;
  }

  /* Curriculum Section */
  .curriculumSection {
    padding: 2rem 0;
  }

  .sectionTitle {
    font-size: 1.8rem;
  }

  .curriculumGrid {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }

  .curriculumItem {
    padding: 1.5rem;
  }

  /* Features Section */
  .featuresSection {
    padding: 2rem 0;
  }

  .featuresGrid {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }

  .featureCard {
    padding: 1.5rem;
  }

  /* Capstone Section */
  .capstoneSection {
    padding: 2rem 0;
  }

  .capstoneContent {
    flex-direction: column;
    gap: 2rem;
  }

  .capstoneText {
    max-width: 100%;
  }

  .capstoneIllustration {
    min-width: 100%;
    margin-top: 1rem;
  }

  .capstoneRobot {
    max-width: 200px;
  }
}

@media (max-width: 480px) {
  .mainTitle {
    font-size: 1.8rem;
  }

  .subTitle {
    font-size: 1.1rem;
  }

  .robotSvg {
    max-width: 250px; /* Further reduced for small screens */
  }

  .sectionTitle {
    font-size: 1.5rem;
  }

  .capstoneTitle {
    font-size: 1.5rem;
  }

  .buttons {
    gap: 0.8rem;
  }
}

/* Extra small screens */
@media (max-width: 360px) {
  .mainTitle {
    font-size: 1.5rem;
  }

  .subTitle {
    font-size: 1rem;
  }

  .robotSvg {
    max-width: 200px; /* Even smaller for very small screens */
  }

  .heroBanner {
    padding: 1.5rem 0.5rem;
  }
}

/* Hackathon Information Section */
.hackathonSection {
  padding: 4rem 0;
  background: linear-gradient(135deg, #0e2a47 0%, #1a3c5f 100%);
  color: #e0f2fe;
}

.hackathonContent {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 3rem;
  width: 100%;
}

@media screen and (max-width: 996px) {
  .hackathonContent {
    flex-direction: column;
    text-align: center;
    gap: 2rem;
  }
}

.hackathonText {
  flex: 1;
  max-width: 800px;
}

/* Circular Curriculum Section */
.circularSection {
  padding: 4rem 0;
  background: linear-gradient(135deg, #2d1b69 0%, #4c1d95 100%);
  color: #e0e7ff;
}

.circularContainer {
  position: relative;
  width: 600px;
  height: 600px;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}

.circularTrack {
  position: relative;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.circularItem {
  position: absolute;
  width: 120px;
  height: 120px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(255, 255, 255, 0.1);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 50%;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);
  backdrop-filter: blur(10px);
}

.circularItem:hover {
  transform: scale(1.2) !important;
  box-shadow: 0 8px 16px rgba(236, 72, 153, 0.4);
  border-color: #f0abfc;
}

.circularItemContent {
  text-align: center;
  padding: 1rem;
}

.circularItemContent h4 {
  color: #f0abfc;
  margin: 0.5rem 0;
  font-size: 0.9rem;
  font-family: 'Helvetica Neue', Arial, sans-serif;
}

.circularItemContent p {
  color: #c7d2fe;
  margin: 0;
  font-size: 0.8rem;
}

.centerCircle {
  position: absolute;
  width: 150px;
  height: 150px;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  border: 2px solid rgba(255, 255, 255, 0.3);
  z-index: 1;
  backdrop-filter: blur(10px);
}

.centerCircle h3 {
  color: #f0abfc;
  margin: 0;
  font-size: 1.2rem;
  font-family: 'Courier New', Courier, monospace;
}

.centerCircle p {
  color: #c7d2fe;
  margin: 0;
  font-size: 0.8rem;
}

/* Skills Section */
.skillsSection {
  padding: 4rem 0;
  background: linear-gradient(135deg, #0f3057 0%, #1e3a8a 100%);
  color: #dbeafe;
}

.skillsContainer {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
}

.skillItem {
  width: 180px;
  background: rgba(255, 255, 255, 0.05);
  border: 1px solid rgba(255, 255, 255, 0.15);
  border-radius: 8px;
  padding: 1.5rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.2);
  backdrop-filter: blur(5px);
}

.skillItem:hover {
  transform: scale(1.1) rotate(5deg);
  box-shadow: 0 10px 20px rgba(96, 165, 250, 0.3);
  border-color: #93c5fd;
}

.skillIcon {
  font-size: 2rem;
  margin-bottom: 1rem;
  color: #93c5fd;
}

.skillItem h4 {
  color: #bfdbfe;
  margin: 1rem 0 0.5rem 0;
  font-size: 1rem;
  font-family: 'Arial Rounded MT Bold', 'Helvetica Rounded', Arial, sans-serif;
}

.skillBar {
  width: 100%;
  height: 8px;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 4px;
  margin: 0.5rem 0;
  overflow: hidden;
}

.skillBarFill {
  height: 100%;
  background: linear-gradient(90deg, #38bdf8, #93c5fd);
  border-radius: 4px;
  transition: width 0.5s ease;
}

.skillLevel {
  color: #94a3b8;
  font-size: 0.8rem;
  font-weight: 500;
}

/* Timeline Section */
.timelineSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.timelineContainer {
  width: 100%;
  overflow-x: auto;
  padding: 2rem 0;
  -ms-overflow-style: auto;
  scrollbar-width: thin;
}

.timelineContainer::-webkit-scrollbar {
  height: 8px;
}

.timelineContainer::-webkit-scrollbar-track {
  background: rgba(255,255,255,0.1);
  border-radius: 10px;
}

.timelineContainer::-webkit-scrollbar-thumb {
  background: rgba(128, 128, 128, 0.5);
  border-radius: 10px;
}

.timelineContainer::-webkit-scrollbar-thumb:hover {
  background: rgba(128, 128, 128, 0.7);
}

.timelineTrack {
  display: flex;
  gap: 2rem;
  min-width: max-content;
}

.timelineItem {
  min-width: 300px;
  background: var(--ifm-color-background-card);
  border: 1px solid var(--ifm-color-border);
  border-radius: 8px;
  padding: 1.5rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  margin: 0 15px;
}

.timelineItem:hover {
  transform: translateY(-10px);
  box-shadow: 0 10px 20px rgba(0, 168, 255, 0.2);
  border-color: var(--ifm-color-primary-light);
}

.timelineBadge {
  display: inline-block;
  background: var(--ifm-color-primary);
  color: white;
  padding: 0.3rem 0.8rem;
  border-radius: 20px;
  font-size: 0.8rem;
  font-weight: 600;
  margin-bottom: 1rem;
}

.timelineItem h4 {
  color: var(--ifm-color-primary);
  margin: 1rem 0 0.5rem 0;
  font-size: 1.1rem;
  font-family: var(--ifm-font-family-base);
}

.timelineItem p {
  color: var(--ifm-color-text-secondary);
  margin: 0;
  font-size: 0.9rem;
  line-height: 1.4;
}

/* Curriculum Section */
.curriculumSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.sectionHeader {
  text-align: center;
  margin-bottom: 3rem;
}

.sectionTitle {
  font-family: var(--ifm-font-family-base);
  font-size: 2rem;
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  text-transform: uppercase;
  letter-spacing: 1.5px;
}

.sectionSubtitle {
  color: var(--ifm-color-text-secondary);
  font-size: 1.1rem;
}

.curriculumGrid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 2rem;
  max-width: 1400px;
  margin: 0 auto;
}

.curriculumItem {
  background: var(--ifm-color-background-card);
  border: 1px solid var(--ifm-color-border);
  border-radius: 8px;
  padding: 2rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.curriculumItem:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(0, 168, 255, 0.2);
  border-color: var(--ifm-color-primary-light);
}

.curriculumIcon {
  font-size: 3rem;
  margin-bottom: 1rem;
  color: var(--ifm-color-primary-light);
}

.curriculumItem h3 {
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  font-family: var(--ifm-font-family-base);
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 1.1rem;
}

.curriculumItem p {
  color: var(--ifm-color-text-secondary);
  margin: 0;
  font-size: 0.95rem;
  line-height: 1.5;
}

/* Learning Outcomes Section */
.outcomesSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.outcomesGrid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
}

.outcomeItem {
  background: var(--ifm-color-background-card);
  border: 1px solid var(--ifm-color-border);
  border-radius: 8px;
  padding: 2rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.outcomeItem:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(0, 168, 255, 0.2);
  border-color: var(--ifm-color-primary-light);
}

.outcomeIcon {
  font-size: 3rem;
  margin-bottom: 1rem;
  color: var(--ifm-color-primary-light);
}

.outcomeItem h3 {
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  font-family: var(--ifm-font-family-base);
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 1.1rem;
}

.outcomeItem p {
  color: var(--ifm-color-text-secondary);
  margin: 0;
  font-size: 0.95rem;
}

/* Features Section */
.featuresSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.featuresGrid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
}

.featureCard {
  background: var(--ifm-color-background-card);
  border: 1px solid var(--ifm-color-border);
  border-radius: 8px;
  padding: 2rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.featureCard:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(0, 168, 255, 0.2);
  border-color: var(--ifm-color-primary-light);
}

.featureIcon {
  font-size: 3rem;
  margin-bottom: 1rem;
  color: var(--ifm-color-primary-light);
}

.featureCard h3 {
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  font-family: var(--ifm-font-family-base);
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 1.2rem;
}

.featureCard p {
  color: var(--ifm-color-text-secondary);
  margin: 0;
}

/* Hardware Requirements Section */
.hardwareSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.hardwareGrid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
}

.hardwareItem {
  background: var(--ifm-color-background-card);
  border: 1px solid var(--ifm-color-border);
  border-radius: 8px;
  padding: 2rem;
  text-align: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.hardwareItem:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 20px rgba(0, 168, 255, 0.2);
  border-color: var(--ifm-color-primary-light);
}

.hardwareIcon {
  font-size: 3rem;
  margin-bottom: 1rem;
  color: var(--ifm-color-primary-light);
}

.hardwareItem h3 {
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  font-family: var(--ifm-font-family-base);
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 1.1rem;
}

.hardwareItem p {
  color: var(--ifm-color-text-secondary);
  margin: 0.5rem 0;
  font-size: 0.95rem;
  text-align: left;
}


/* Capstone Section */
.capstoneSection {
  padding: 4rem 0;
  background-color: var(--ifm-color-background);
  color: var(--ifm-color-text);
}

.capstoneContent {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 3rem;
  width: 100%;
}

@media screen and (max-width: 996px) {
  .capstoneContent {
    flex-direction: column;
    text-align: center;
    gap: 2rem;
  }
}

.capstoneText {
  flex: 1;
  max-width: 600px;
}

.capstoneTitle {
  font-family: var(--ifm-font-family-base);
  font-size: 2rem;
  color: var(--ifm-color-primary);
  margin-bottom: 1rem;
  text-transform: uppercase;
  letter-spacing: 1.5px;
}

.capstoneDescription {
  color: var(--ifm-color-text-secondary);
  font-size: 1.1rem;
  margin-bottom: 2rem;
  line-height: 1.6;
}

.capstoneFeatures {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
  gap: 1rem;
  margin-bottom: 2rem;
}

.capstoneFeature {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  padding: 0.8rem;
  background: var(--ifm-color-background);
  border-radius: 6px;
  text-align: center;
}

.capstoneFeature .featureIcon {
  font-size: 1.2rem;
  margin: 0;
}

.capstoneIllustration {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  min-width: 300px;
  min-height: 300px;
}

@media screen and (max-width: 996px) {
  .capstoneIllustration {
    min-width: 100%;
    margin-top: 1rem;
  }
}

.capstoneRobot {
  width: 100%;
  max-width: 250px;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.capstoneSvg {
  width: 100%;
  height: auto;
  filter: drop-shadow(0 10px 15px rgba(0, 168, 255, 0.3));
}

/* Call to Action Section */
.ctaSection {
  padding: 4rem 0;
  background: linear-gradient(135deg, var(--ifm-color-primary) 0%, var(--ifm-color-primary-dark) 100%);
  color: white;
  text-align: center;
}

.ctaContent {
  max-width: 800px;
  margin: 0 auto;
}

.ctaTitle {
  font-family: var(--ifm-font-family-base);
  font-size: 2.5rem;
  margin-bottom: 1rem;
  text-transform: uppercase;
  letter-spacing: 1.5px;
}

.ctaDescription {
  font-size: 1.2rem;
  margin-bottom: 2rem;
  opacity: 0.9;
}

.ctaButtons {
  display: flex;
  gap: 1rem;
  justify-content: center;
  flex-wrap: wrap;
}

@media screen and (max-width: 768px) {
  .ctaButtons {
    flex-direction: column;
    align-items: center;
  }
}

/* Responsive adjustments */
@media screen and (max-width: 996px) {
  .curriculumGrid,
  .featuresGrid,
  .outcomesGrid,
  .hardwareGrid {
    grid-template-columns: 1fr;
  }

  .sectionTitle {
    font-size: 1.5rem;
  }

  .capstoneTitle {
    font-size: 1.5rem;
  }

  .circularContainer {
    width: 400px;
    height: 400px;
  }

  .skillsContainer {
    flex-direction: column;
    align-items: center;
  }

  .skillItem {
    width: 100%;
    max-width: 300px;
  }
}
